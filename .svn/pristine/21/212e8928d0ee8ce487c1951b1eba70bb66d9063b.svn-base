  <!--根据门店id查询妈妈医师查房记录表-->
  <select id="selectPatientroundsBystoreId"  resultMap="BaseResultMap">
    select p.id, p.customer_basic_id, p.operationDate,p.shrinkage, p.cesareanSection, p.sideCutter,
      p.lochiaColor, p.lochiaSmell, p.leucorrhea,p.fever, p.signature, p.otherSituations,
      p.motherId, p.store_id,p.type,c.name as customerName
    from patientrounds p LEFT JOIN customer_basic c ON p.customer_basic_id=c.id
    where p.store_id = #{storeId}
  </select>
