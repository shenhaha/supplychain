<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<c:set var="ctx" value="${pageContext.request.contextPath}" />
<html>
<head>
    <title>排房信息页面</title>
</head>
<body>
    <table id="scheduleList" class="table" data-paging="true" data-paging-position="right"></table>
    <script >
        $(function () {
            var scheduleTitle = [
                {"name":"id","title":"编号",visible:false,"breakpoints":"xs sm","type":"number","style":{"width":80,"maxWidth":80}},
                {"name":"institution","title":"所在机构"},
                {"name":"department","title":"所在部门"},
                {"name":"type","title":"班次类型"},
                {"name":"detailinfo","title":"外派和医陪备注"},
                {"name":"starttime","title":"开始时间"},
                {"name":"endtime","title":"结束时间"},
                {"name":"employid","title":"登记时间"},
                {"name":"createdate","title":"创建日期"}
            ];
            $('#scheduleList').footable({
                "columns": scheduleTitle,
                "rows": $.get("${ctx}/schedule/selectScheduleAll")
            });
        });
    </script>
</body>
</html>
