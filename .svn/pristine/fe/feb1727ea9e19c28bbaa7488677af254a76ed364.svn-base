package com.luer.storemanager.bean;

import lombok.Data;
import org.springframework.format.annotation.DateTimeFormat;

import java.util.Date;

/**
 * 排班关联查询表
 *
 * @ClassName ScheduleList
 * @Author GavinJiang
 * @Date 2018/8/29 11:48.
 */
@Data
public class ScheduleList {
    private String id; //排班表id

    private String institution; //所在机构

    private String department; //所在部门

    private String type; //班次

    private String detailinfo; //备注

    @DateTimeFormat(pattern = "yyyy-MM-dd")
    private Date starttime; //开始时间

    @DateTimeFormat(pattern = "yyyy-MM-dd")
    private Date endtime; //结束时间

    private String employid; //员工id

    private String employ_Name; //员工名称

    private String store_Name; //所属店铺名称

    @DateTimeFormat(pattern = "yyyy-MM-dd")
    private Date createdate; //创建日期
}
