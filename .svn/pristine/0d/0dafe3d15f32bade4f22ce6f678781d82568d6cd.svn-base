package com.luer.storesdata.bean;


import org.springframework.web.multipart.MultipartFile;

import java.util.List;

public class FilesOb {

	private List<MultipartFile> uploadFiles;//上传的文件对象列表
	private String tableName;//子文件夹,全路径所要存入的表名
	private String[] fileUrls;//除了根目录下的全路径,按照存入文件的顺序存入数组
	private String DBName;//返回的全路径需要存入的那个数据库代码
	
	public String getDBName() {
		return DBName;
	}
	public void setDBName(String dBName) {
		DBName = dBName;
	}
	public List<MultipartFile> getUploadFiles() {
		return uploadFiles;
	}
	public void setUploadFiles(List<MultipartFile> uploadFiles) {
		this.uploadFiles = uploadFiles;
	}

	public String getTableName() {
		return tableName;
	}
	public void setTableName(String tableName) {
		this.tableName = tableName;
	}
	
	public String[] getFileUrls() {
		return fileUrls;
	}
	public void setFileUrls(String[] fileUrls) {
		this.fileUrls = fileUrls;
	}
	
	
}
