    <select id="selectNotice" resultMap="BaseResultMap" parameterType="com.luer.businessmanager.bean.OrderCustomer">
        select oc.id id, oc.store_id, oc.customer_id, first_party, card_id, address, tell, medical_history, intime,
        days, born_times, room_type, room_number, price, bargain_money, pay_way, final_payment, final_payment_status, operator,
        remark, fathername, fathertell
        from order_customer oc LEFT JOIN  customer_basic cc ON
        cc.id = oc.customer_id
        WHERE cc.type = '0' AND intime BETWEEN #{starttime} and #{endtime} and cc.check_status='0'  and oc.store_id = #{orderCustomer.storeId}
    </select>
    <update id="updateStatusByMotherid">
