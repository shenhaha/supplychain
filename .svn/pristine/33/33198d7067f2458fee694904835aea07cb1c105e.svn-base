                    $("#motherId").append("<option  value='" + data[i].id + "'>" + data[i].name + "</option>");
                }
            }
        });


    });


    //载入柜台服务质量题目
    function getCSSFHtml(data) {
        if (data == undefined) return;
        var CSSFHtml = "";
        var count = 0;
        for (var i = 0; i < data.length; i++) {
            if (data[i].QuestionBankEntity.QuestionType == "CSSF") {
                //
                CSSFHtml += '<div class="question" >';
                CSSFHtml += '<h4 id="' + data[i].QuestionBankEntity.QuestionBankId + '" class="questionContent">' + (count + 1) + '.' + data[i].QuestionBankEntity.QuestionContent + '</h4>';
                CSSFHtml += '<div class="questionOption">'
                var questionList = data[i].QuestionChooseList;
                //选项内容
                for (var j = 0; j < questionList.length; j++) {
