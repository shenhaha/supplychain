<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<c:set var="ctx" value="${pageContext.request.contextPath}"/>
<html>
<head>
    <link rel="stylesheet" href="${ctx}/static/template/bootstrap.min.css">
    <link rel="stylesheet" href="${ctx}/static/css/bootstrapValidator.css"/>
    <!-- 最新的 Bootstrap 核心 JavaScript 文件 -->
    <script src="${ctx}/static/template/bootstrap.min.js"></script>
    <script type="text/javascript" src="${ctx}/static/js/bootstrapValidator.js"></script>
    <%--日期时间插件--%>
    <script src="${ctx}/static/js/bootstrap-datetimepicker.js"></script>
    <script src="${ctx}/static/js/bootstrap-datetimepicker.zh-CN.js"></script>
</head>
<body>
<form class="form-horizontal" id="myform" role="form">
    <div class="form-group required" hidden="hidden">
        <label for="id" class="col-sm-2 control-label" style="margin: 0 0;padding: 0 0">ID</label>
        <div class="col-sm-9">
            <input type="text" class="form-control" id="id" name="id" placeholder="id">
        </div>
    </div>

    <div class="container">

        <div class="row">
            <div class="form-group col-md-4" style="margin-bottom: 10px">
                <label for="dtp_input1" class="col-xs-4 control-label">评估时间:</label>
                <div style="padding-left: 7.5px;padding-right: 7.5px" class="input-group date form_datetime col-xs-7"
                     data-date="" data-date-format="yyyy-mm-dd hh:ii:ss" data-link-field="dtp_input1">
                    <input class="form-control" size="16" type="text" value="" name="assessmentTime" id="assessmentTime"
                           readonly>
                    <span class="input-group-addon"><span class="glyphicon glyphicon-th"></span></span>
                </div>
                <input type="hidden" id="dtp_input1" value=""/><br/>
            </div>

            <div class="form-group col-xs-3">
                <label for="assessment"
                       class="col-xs-5 control-label">评估人:</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="assessment" name="assessment" placeholder="评估人" required>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="form-group col-md-4">
                <label for="badNo" class="col-xs-4 control-label">床号:</label>
                <div class="col-xs-6">
                    <input type="text" class="form-control" id="badNo" name="badNo" placeholder="床号" required>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="childTime" class="col-xs-5 control-label">胎次:</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="childTime"
                           name="childTime" placeholder="胎次" required>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="weightIncrease" class="col-xs-5 control-label">体重增加(kg):</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="weightIncrease" name="weightIncrease" placeholder="体重增加">
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="childBirthWay"
                       class="col-xs-5 control-label">分娩方式:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="childBirthWay" name="childBirthWay" tabindex="1">
                        <option value="自然生产">自然生产</option>
                        <option value="剖腹产">剖腹产</option>
                    </select>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="form-group col-md-4">
                <label for="pregnancy" class="col-xs-4 control-label">妊娠合并症:</label>
                <div class="col-xs-6">
                    <select type="text" class="form-control" id="pregnancy" name="pregnancy" tabindex="1">
                        <option value="无">无</option>
                        <option value="妊娠糖尿症">妊娠糖尿症</option>
                        <option value="高血压">高血压</option>
                        <option value="前置胎盘">前置胎盘</option>
                        <option value="胎盘早期剖离">胎盘早期剖离</option>
                    </select>
                </div>
            </div>

            <div class="form-group col-xs-3">
                <label for="tear" class="col-xs-5 control-label">撕裂伤:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="tear" name="tear" tabindex="1">
                        <option value="0">0</option>
                        <option value="Ⅰ">Ⅰ</option>
                        <option value="Ⅱ">Ⅱ</option>
                        <option value="III">III</option>
                        <option value="Ⅳ">Ⅳ</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3" hidden="hidden" id="fortear" style="margin-left: 2%">
                <label class="col-xs-4 checkbox-inline">
                    <input type="checkbox"  value="左切"  name="tear"> 左切
                </label>
                <label class="col-xs-3 checkbox-inline">
                    <input type="checkbox"  value="右切"  name="tear"> 右切
                </label>
            </div>
            <div class="form-group col-xs-3"  style="margin-left:22%;">
                <label for="pph" class="col-xs-5 control-label">PPH:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="pph" name="pph" tabindex="1">
                        <option value="有">有</option>
                        <option value="无">无</option>
                    </select>
                </div>
            </div>
        </div>

    </div>
        <div class="row">
            <div class="form-group col-md-4">
                <label for="t" class="col-xs-4 control-label">t:</label>
                <div class="col-xs-6">
                    <input type="text" class="form-control" id="t" name="t" placeholder="t" required>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="p" class="col-xs-5 control-label">p:</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="p" name="p" placeholder="p" required>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="r" class="col-xs-5 control-label">r:</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="r" name="r" placeholder="r" required>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="bp" class="col-xs-5 control-label">bp:</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="bp" name="bp" placeholder="bp" required>
                </div>
            </div>
        </div>
        <div class="row">


            <div class="form-group col-md-4">
                <label for="uterusHeight"
                       class="col-xs-4 control-label">子宫底高度:</label>
                <div class="col-xs-6">
                    <select type="text" class="form-control" id="uterusHeight" name="uterusHeight" tabindex="1">
                        <option value="脐上1指">脐上1指(u+1/Fb)</option>
                        <option value="脐平">脐平(u)</option>
                        <option value="脐下1指">脐下1指(u-/Fb)</option>
                        <option value="脐下2指">脐下2指(u-/2Fb)</option>
                        <option value="脐下3指">脐下3指(u-/3Fb)</option>
                        <option value="脐下4指">脐下4指(u-4Fb)</option>
                        <option value="入盆">入盆</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="uterusHardness"
                       class="col-xs-5 control-label">子宫底软硬度:</label>
                <div class="col-xs-7">
                    <input type="text" class="form-control" id="uterusHardness"
                           name="uterusHardness" placeholder="子宫底软硬度" required>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="lochia" class="col-xs-5 control-label">恶露:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="lochia" name="lochia" tabindex="1">
                        <option value="量多">量多</option>
                        <option value="量中">量中</option>
                        <option value="量少">量少</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="color" class="col-xs-5 control-label">恶露颜色:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="color" name="color" tabindex="1">
                        <option value="鲜红">鲜红</option>
                        <option value="暗红">暗红</option>
                        <option value="褐色">褐色</option>
                        <option value="浅褐色">浅褐色</option>
                        <option value="黄白色">黄白色</option>
                    </select>
                </div>
            </div>


        </div>
        <div class="row">


            <div class="form-group col-md-4">
                <label for="taste" class="col-xs-4 control-label">恶露味道:</label>
                <div class="col-xs-6">
                    <select type="text" class="form-control" id="taste" name="taste" tabindex="1">
                        <option value="无">无</option>
                        <option value="腥味">腥味</option>
                        <option value="臭味">臭味</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label  class="col-xs-5 control-label" style="margin-left: 25px">有无血块:</label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="有"  name="lochia"> 有
                </label>
                <label class="col-sm-2 checkbox-inline">
                    <input type="checkbox"  value="无"  name="lochia"> 无
                </label>
            </div>
            <div class="form-group col-xs-3">
                <label for="perineumWound" class="col-xs-5 control-label">会阴伤口:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="perineumWound" name="perineumWound" tabindex="1">
                        <option value="Clear">Clear</option>
                        <option value="红">红</option>
                        <option value="肿">肿</option>
                        <option value="灼热">灼热</option>
                        <option value="痛">痛</option>
                        <option value="渗血">渗血</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="cesareanWound" class="col-xs-5 control-label">剖腹产伤口:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="cesareanWound" name="cesareanWound" tabindex="1">
                        <option value="Clear">Clear</option>
                        <option value="红">红</option>
                        <option value="肿">肿</option>
                        <option value="灼热">灼热</option>
                        <option value="痛">痛</option>
                        <option value="渗血">渗血</option>
                    </select>
                </div>
            </div>

        </div>
        <div class="row">


            <%--<div class="form-group col-md-3">
                <label for="character1"
                       class="col-sm-4 control-label">性状</label>
                <div class="col-sm-8">
                    <select type="text" class="form-control" id="character1" name="character1" tabindex="1">
                        <option value="稀薄">稀薄</option>
                        <option value="黏稠">黏稠</option>
                        <option value="血块">血块</option>
                    </select>
                </div>
            </div>--%>

        </div>
        <div class="row">
            <div class="form-group col-md-12">
                <label  class="col-sm-1 control-label" style="margin-left: 28px">乳房:</label>
                <label class="col-sm-1 checkbox-inline" style="margin-left: 8px">
                    <input type="checkbox"  value="柔软"  name="breast"> 柔软
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="充盈"  name="breast"> 充盈
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox" value="肿胀"  name="breast"> 肿胀
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="硬块"  name="breast"> 硬块
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="疼痛"  name="breast"> 疼痛
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="副乳"  name="breast"> 副乳
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="双侧"  name="breast"> 双侧
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="左侧"  name="breast"> 左侧
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="右侧"  name="breast"> 右侧
                </label>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-12">
                <label  class="col-sm-1 control-label" style="margin-left: 28px">乳头:</label>
                <label class="col-sm-1 checkbox-inline" style="margin-left: 8px">
                    <input type="checkbox"  value="正常"  name="nipple"> 正常
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="凹陷"  name="nipple"> 凹陷
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox" value="短小"  name="nipple"> 短小
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="破裂"  name="nipple"> 破裂
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="双侧"  name="nipple"> 双侧
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="左侧"  name="nipple"> 左侧
                </label>
                <label class="col-sm-1 checkbox-inline">
                    <input type="checkbox"  value="右侧"  name="nipple"> 右侧
                </label>
            </div>
        </div>
        <div class="row">
            <%--<div class="form-group col-md-4">
                <label for="breast"
                       class="col-sm-4 control-label">乳房</label>
                <div class="col-sm-7">
                    <select type="text" class="form-control" id="breast" name="breast" tabindex="1">
                        <option value="柔软">柔软</option>
                        <option value="肿胀">肿胀</option>
                        <option value="硬块">硬块</option>
                        <option value="疼痛">疼痛</option>
                        <option value="皮肤破裂">皮肤破裂</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-md-3">
                <label for="nipple"
                       class="col-sm-4 control-label">乳头</label>
                <div class="col-sm-7">
                    <select type="text" class="form-control" id="nipple" name="nipple" tabindex="1">
                        <option value="正常">正常</option>
                        <option value="凹陷">凹陷</option>
                        <option value="短小">短小</option>
                        <option value="破裂">破裂</option>
                    </select>
                </div>
            </div>--%>
            <div class="form-group col-md-4">
                <label for="mastitis"
                       class="col-xs-4 control-label">乳腺炎:</label>
                <div class="col-xs-6">
                    <select type="text" class="form-control" id="mastitis" name="mastitis" tabindex="1">
                        <option value="有">有</option>
                        <option value="无">无</option>
                    </select>
                </div>
            </div>

            <div class="form-group col-xs-3">
                <label for="secretion"
                       class="col-xs-5 control-label">乳汁分泌:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="secretion" name="secretion" tabindex="1">
                        <option value="无">无</option>
                        <option value="少量">少量</option>
                        <option value="中量">中量</option>
                        <option value="多量">多量</option>
                    </select>
                </div>
            </div>

            <div class="form-group col-xs-3">
                <label for="hemorrhoids"
                       class="col-xs-5 control-label">痔疮:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="hemorrhoids" name="hemorrhoids" tabindex="1">
                        <option value="无">无</option>
                        <option value="有">有</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="defecation"
                       class="col-xs-5 control-label">排便情况:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="defecation" name="defecation" tabindex="1">
                        <option value="正常">正常</option>
                        <option value="不正常">不正常</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-4">
                <label for="infectiousDiseases"
                       class="col-xs-4 control-label">传染病:</label>
                <div class="col-xs-6">
                    <select type="text" class="form-control" id="infectiousDiseases" name="infectiousDiseases"
                            tabindex="1">
                        <option value="无">无</option>
                        <option value="B肝">B肝</option>
                        <option value="梅毒血清">梅毒血清</option>
                        <option value="HIV">HIV</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="extraMedications"
                       class="col-xs-5 control-label">备用药物:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="extraMedications" name="extraMedications" tabindex="1">
                        <option value="无">无</option>
                        <option value="有">有</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="edemaState" class="col-xs-5 control-label">水肿状态:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="edemaState" name="edemaState" tabindex="1">
                        <option value="全身">全身</option>
                        <option value="下肢">下肢</option>
                        <option value="脸部">脸部</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="mentalState" class="col-xs-5 control-label">精神状态:</label>
                <div class="col-xs-7">
                    <select type="text" class="form-control" id="mentalState" name="mentalState" tabindex="1">
                        <option value="正常">正常</option>
                        <option value="疲倦">疲倦</option>
                        <option value="虚弱">虚弱</option>
                        <option value="苍白">苍白</option>
                    </select>
                </div>
            </div>

        </div>
    </div>

    <%--<div class="row">
        <div class="form-group col-md-4">
            <label for="mentalState" class="col-xs-4 control-label">精神状态:</label>
            <div class="col-xs-6">
                <select type="text" class="form-control" id="mentalState" name="mentalState" tabindex="1">
                    <option value="正常">正常</option>
                    <option value="疲倦">疲倦</option>
                    <option value="虚弱">虚弱</option>
                    <option value="苍白">苍白</option>
                </select>
            </div>
        </div>--%>
    <div class="row">

            <div class="form-group col-md-4">
                <label for="emotionalState" class="col-xs-4 control-label">情绪状态:</label>
                <div class="col-xs-6">
                    <select type="text" class="form-control" id="emotionalState" name="emotionalState" tabindex="1">
                        <option value="平稳">平稳</option>
                        <option value="冷漠">冷漠</option>
                        <option value="忧伤">忧伤</option>
                        <option value="聒噪不安">聒噪不安</option>
                    </select>
                </div>
            </div>
            <div class="form-group col-xs-3">
                <label for="note" class="col-xs-5 control-label">备注:</label>
                <div class="col-xs-7">
                    <textarea type="text" class="form-control" id="note" name="note" placeholder="备注"></textarea>
                </div>
            </div>

    </div>
</form>
<script>
    $('.form_datetime').datetimepicker({
        language: 'zh-CN',
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        forceParse: 0,
        showMeridian: 1
    });

    $(function () {
        //动态查询妈妈信息
        $.ajax({
            url: "${ctx}/customerBasic/selectCustomerBasicByStoreId?type=0&storeId=" + '<%=session.getAttribute("storeId")%>',
            type: "post",
            success: function (data) {
                for (var i = 0; i < data.length; i++) {
                    $("#mothername").append("<option value='" + data[i].name + "'>" + data[i].name + "</option>");
                }
            }
        });

    });
</script>
</body>
</html>
