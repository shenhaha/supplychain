        });
        //动态查询妈妈信息
        $.ajax({
            url:/customerBasic/selectCustomerBasicByStoreId?type=0&storeId="+'<%=session.getAttribute("storeId")%>',
            type:"post",
            success:function(data){
                for(var i=0;i<data.length;i++){
                    $("#mother").append("<option value='"+data[i].name+"'>"+data[i].name+"</option>");
                }
            }
        });

    });*/
</script>
</body>
</html>

