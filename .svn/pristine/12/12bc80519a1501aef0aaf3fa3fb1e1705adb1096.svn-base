<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<c:set var="ctx" value="${pageContext.request.contextPath}" />
<html>
<head>
    <link rel="stylesheet" href="${ctx}/static/template/bootstrap.min.css">
    <link rel="stylesheet" href="${ctx}/static/css/bootstrapValidator.css"/>
    <!-- 最新的 Bootstrap 核心 JavaScript 文件 -->
    <script src="${ctx}/static/template/bootstrap.min.js"></script>
    <script type="text/javascript" src="${ctx}/static/js/bootstrapValidator.js"></script>
    <%--日期时间插件--%>
    <script src="${ctx}/static/js/bootstrap-datetimepicker.js"></script>
    <script src="${ctx}/static/js/bootstrap-datetimepicker.zh-CN.js"></script>
</head>
<body>
<form class="form-horizontal" role="form" id="myform">
    <div class="form-group required" hidden="hidden">
        <label for="id" class="col-sm-2 control-label" style="margin: 0 0;padding: 0 0">ID</label>
        <div class="col-sm-9">
            <input type="text" class="form-control" id="id" name="id" placeholder="id" >
        </div>
    </div>
    <div class="container">
        <div class="form-group">
            <label for="answerone" class="col-sm-3 control-label">我有能支持我的好朋友:</label>
            <div class="col-sm-5">
                <select  type="text" class="form-control" id="answerone" name="answerone" placeholder="我有能支持我的好朋友" tabindex="1">
                    <option value="从没有">从没有</option>
                    <option value="很少">很少</option>
                    <option value="偶尔">偶尔</option>
                    <option value="时常">时常</option>
                    <option value="总是">总是</option>
                </select>
            </div>
        </div>

        <div class="form-group ">
            <label for="answertwo" class="col-sm-3 control-label">当我有需要时，我的家人可以提供协助 :</label>
            <div class="col-sm-5">
                <select  type="text" class="form-control" id="answertwo" name="answertwo" placeholder="当我有需要时，我的家人可以提供协助" tabindex="1">
                    <option value="从没有">从没有</option>
                    <option value="很少">很少</option>
                    <option value="偶尔">偶尔</option>
                    <option value="时常">时常</option>
                    <option value="总是">总是</option>
                </select>
            </div>
        </div>
        <div class="form-group ">
            <label for="answerthree" class="col-sm-3 control-label">我的先生/伴侣可以帮助我许多事情 :</label>
            <div class="col-sm-5">
                <select  type="text" class="form-control" id="answerthree" name="answerthree" placeholder="我的先生/伴侣可以帮助我许多事情" tabindex="1">
                    <option value="从没有">从没有</option>
                    <option value="很少">很少</option>
                    <option value="偶尔">偶尔</option>
                    <option value="时常">时常</option>
                    <option value="总是">总是</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for="answerfour" class="col-sm-3 control-label">我与我的先生/伴侣之间有冲突 :</label>
            <div class="col-sm-5">
                <select  type="text" class="form-control" id="answerfour" name="answerfour" placeholder="我与我的先生/伴侣之间有冲突" tabindex="1">
                    <option value="从没有">从没有</option>
                    <option value="很少">很少</option>
                    <option value="偶尔">偶尔</option>
                    <option value="时常">时常</option>
                    <option value="总是">总是</option>
                </select>
            </div>
        </div>
        <div class="form-group ">
            <label for="answerfive" class="col-sm-3 control-label">我有感觉被先生/伴侣所控制，而失去自己 :</label>
            <div class="col-sm-5">
                <select  type="text" class="form-control" id="answerfive" name="answerfive" placeholder="我有感觉被先生/伴侣所控制，而失去自己" tabindex="1">
                    <option value="从没有">从没有</option>
                    <option value="很少">很少</option>
                    <option value="偶尔">偶尔</option>
                    <option value="时常">时常</option>
                    <option value="总是">总是</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for="answersix" class="col-sm-3 control-label">我能感觉到先生/伴侣对我的爱 :</label>
            <div class="col-sm-5">
                <select  type="text" class="form-control" id="answersix" name="answersix" placeholder="我能感觉到先生/伴侣对我的爱" tabindex="1">
                    <option value="从没有">从没有</option>
                    <option value="很少">很少</option>
                    <option value="偶尔">偶尔</option>
                    <option value="时常">时常</option>
                    <option value="总是">总是</option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for="answersix" class="col-sm-3 control-label">备注:</label>
            <div class="col-sm-5">
                <input type="text" class="form-control" id="note">
            </div>
        </div>
    </div>
</form>
<script>
    $('.form_datetime').datetimepicker({
        language: 'zh-CN',
        weekStart: 1,
        todayBtn: 1,
        autoclose: 1,
        todayHighlight: 1,
        startView: 2,
        forceParse: 0,
        showMeridian: 1
    });
</script>
</body>
</html>

